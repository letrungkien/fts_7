<% provide(:title, "Edit level") %>
<h1>Update Level</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for [:admin, @level] do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
